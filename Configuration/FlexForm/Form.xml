                    <label>LLL:EXT:koning_mailchimp_signup/Resources/Private/Language/locallang_be.xlf:configuration.form.list</label>
                    <config>
                        <type>select</type>
                        <renderType>selectSingle</renderType>
                        <eval>required</eval>
                        <itemsProcFunc>Keizer\KoningMailchimpSignup\Service\TableConfigurationService->addAvailableAudiences</itemsProcFunc>
                    </config>
                </TCEforms>
            </settings.data.list>
            <settings.data.fields>
