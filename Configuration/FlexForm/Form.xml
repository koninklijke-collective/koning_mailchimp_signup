                        <itemsProcFunc>Keizer\KoningMailchimpSignup\Service\TableConfigurationService->addAvailableFormFieldsForSelectedAudience</itemsProcFunc>
                    </config>
                </TCEforms>
            </settings.data.fields>
            <settings.data.successPid>
